<!DOCTYPE html>
<html lang="en">

<head>

  {{> meta }}

  <title>{{title}}</title>

  {{> css-imports }}
</head>

<body id="page-top">

  <!-- Page Wrapper -->
  <div id="wrapper">

  <!-- Navbar include -->
  {{> navbar  }}
    <!-- Content Wrapper -->
    <div id="content-wrapper" class="d-flex flex-column">

      <!-- Main Content -->
      <div id="content">

        {{> top-navbar }}

        <!-- Begin Page Content -->
        <div class="container-fluid">

          {{{content}}}

        </div>
        <!-- /.container-fluid -->

      </div>
      <!-- End of Main Content -->

      {{> footer }}

    </div>
    <!-- End of Content Wrapper -->

  </div>
  <!-- End of Page Wrapper -->

  {{> page-top }}

  {{> logout-modal }}

  {{> js-imports }}
</body>

</html>
